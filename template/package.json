{
  "name": "rctemplate",
  "version": "3.0.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "type-check": "tsc --build plugins/typescript/template/tsconfig.json",
    "pods": "cd ios && pod install",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "cache-clean": "node bin/clean.js"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/netinfo": "^6.0.4",
    "@react-native-firebase/app": "^12.9.3",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-native-picker/picker": "react-native-picker/picker",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^5.x",
    "@reduxjs/toolkit": "^1.6.1",
    "@thecodingmachine/redux-toolkit-wrapper": "2.0.1",
    "@types/redux-logger": "^3.0.7",
    "apisauce": "^2.1.2",
    "axios": "^0.21.4",
    "i18next": "^20.6.1",
    "moment": "^2.29.1",
    "native-base": "^3.2.1",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-i18next": "^11.12.0",
    "react-native": "0.65.1",
    "react-native-config": "^1.4.5",
    "react-native-flipper": "^0.103.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-styles-shorthand": "^1.0.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-navigation": "^3.11.0",
    "react-navigation-animated-switch": "^0.6.4",
    "react-redux": "^7.2.5",
    "reactotron-react-native": "^5.0.0",
    "redux": "^4.1.1",
    "redux-flipper": "^2.0.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^2.0.0",
    "redux-persist": "^6.0.0",
    "reduxsauce": "^1.2.1",
    "seamless-immutable": "^7.1.4",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5",
    "typesafe-actions": "2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.27",
    "@types/react-native": "^0.65.5",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^17.0.1",
    "@types/seamless-immutable": "^7.1.16",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.22.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native)"
    ]
  }
}
